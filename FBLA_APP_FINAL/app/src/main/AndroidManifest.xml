<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.juanrubio.fbla_app_final">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="AlleyCat"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AccountCreationActivity" />
        <activity android:name=".ItemInformationActivity" />
        <activity android:name=".PurchaseActivity" />
        <activity android:name=".MainMenuActivity" />
        <activity android:name=".ItemDonationActivity" />
        <activity android:name=".PurchaseCompletedActivity" />
        <activity
            android:name=".ConfirmDonationActivity"
            android:label="@string/title_activity_confirm_donation"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".FeaturedItemsActivity" />
        <activity android:name=".SearchActivity" />
        <activity android:name=".CartActivity" />
        <activity android:name=".BuyOptionsActivity"></activity>
    </application>

</manifest>